{
  "name": "@tabstack/spark",
  "version": "0.12.0",
  "description": "AI-powered web automation CLI tool",
  "type": "module",
  "engines": {
    "node": "^22.0.0"
  },
  "bin": {
    "spark": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "dev": "NODE_NO_WARNINGS=1 tsx src/index.ts",
