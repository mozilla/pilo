    "@openrouter/ai-sdk-provider": "^1.5.4",
    "ai": "^5.0.121",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.3",
    "eventemitter3": "^5.0.1",
    "liquidjs": "^10.24.0",
