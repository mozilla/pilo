{
  "name": "pilo-server",
  "version": "0.1.0",
  "description": "Web server for Pilo AI-powered web automation",
  "main": "dist/server/src/index.js",
  "types": "dist/server/src/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/src/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run"
  },
  "keywords": [
    "ai",
    "automation",
    "web-server",
    "hono",
    "pilo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@hono/sentry": "^1.2.2",
    "dotenv": "^17.2.3",
    "hono": "^4.11.10",
    "pilo-core": "workspace:*"
  },
  "packageManager": "pnpm@9.0.0"
}
