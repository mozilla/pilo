{
  "name": "spark-server",
  "version": "0.1.0",
  "description": "Web server for Spark AI-powered web automation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "dev": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write . --list-different",
    "format:check": "prettier --check .",
    "check": "pnpm run format && pnpm run typecheck && pnpm test"
  },
  "keywords": [
    "ai",
    "automation",
    "web-server",
    "hono",
    "spark"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.4",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@hono/node-server": "^1.15.0",
    "ai": "^4.3.16",
    "dotenv": "^16.6.1",
    "hono": "^4.8.3",
    "spark": "file:.."
  },
  "packageManager": "pnpm@9.0.0"
}
