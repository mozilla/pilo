{
  "name": "spark",
  "version": "0.10.0",
  "description": "AI-powered web automation library and CLI tool",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^22.0.0"
  },
  "bin": {
    "spark": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --project tsconfig.check.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "spark": "NODE_NO_WARNINGS=1 tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "install-cli": "npm run build && chmod +x dist/cli.js && npm install -g .",
    "format": "prettier --write . --list-different",
    "format:check": "prettier --check .",
    "release": "./scripts/release.sh",
    "check": "pnpm run format && pnpm run typecheck && pnpm test"
  },
  "keywords": [
    "ai",
    "automation",
    "web-scraping",
    "playwright",
    "browser-automation",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.9.1",
    "jsdom": "^27.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.30",
    "@ai-sdk/google-vertex": "^3.0.55",
    "@ai-sdk/openai": "^2.0.64",
    "@ai-sdk/openai-compatible": "^1.0.7",
    "@ghostery/adblocker-playwright": "^2.11.3",
    "@openrouter/ai-sdk-provider": "^1.1.2",
    "@types/turndown": "^5.0.5",
    "ai": "^5.0.87",
    "chalk": "^5.5.0",
    "commander": "^14.0.2",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.3",
    "eventemitter3": "^5.0.1",
    "liquidjs": "^10.24.0",
    "nanoid": "^5.1.5",
    "ollama-ai-provider-v2": "^1.5.1",
    "playwright": "1.56.1",
    "turndown": "^7.2.2",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@9.0.0"
}
