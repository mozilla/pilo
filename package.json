{
  "name": "spark-workspace",
  "version": "0.12.0",
  "description": "Spark monorepo workspace",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^22.0.0"
  },
  "scripts": {
    "clean": "pnpm -r run clean",
    "bundle:aria": "tsx scripts/bundle-aria-tree.ts",
    "build": "pnpm run bundle:aria && pnpm -r run build",
    "typecheck": "pnpm -r run typecheck",
    "prepare": "pnpm run bundle:aria",
    "spark": "pnpm --filter @tabstack/spark run spark",
    "test": "pnpm -r run test",
    "test:watch": "pnpm -r run test:watch",
    "install-cli": "pnpm --filter @tabstack/spark run build && pnpm install -g ./packages/cli",
    "format": "prettier --write . --list-different",
    "format:check": "prettier --check .",
    "release": "./scripts/release.sh",
    "check": "pnpm run format && pnpm run typecheck && pnpm run test"
  },
  "keywords": [
    "ai",
    "automation",
    "web-scraping",
    "playwright",
    "browser-automation",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.2.0",
    "@types/turndown": "^5.0.6",
    "@vitest/coverage-v8": "^4.0.18",
    "esbuild": "^0.27.3",
    "jsdom": "^28.0.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.20",
    "@ai-sdk/google-vertex": "^4.0.41",
    "@ai-sdk/openai": "^3.0.25",
    "@ai-sdk/openai-compatible": "^2.0.26",
    "@ghostery/adblocker-playwright": "^2.13.4",
    "@openrouter/ai-sdk-provider": "^2.1.1",
    "ai": "^6.0.69",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.3",
    "eventemitter3": "^5.0.4",
    "liquidjs": "^10.24.0",
    "nanoid": "^5.1.6",
    "ollama-ai-provider-v2": "^3.0.3",
    "playwright": "1.58.1",
    "turndown": "^7.2.2",
    "zod": "^4.3.6"
  },
  "packageManager": "pnpm@9.0.0"
}
