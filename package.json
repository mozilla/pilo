{
  "name": "spark",
  "version": "0.12.1",
  "description": "AI-powered web automation library and CLI tool",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^22.0.0"
  },
  "bin": {
    "spark": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./ariaTree": {
      "types": "./dist/browser/ariaTree/index.d.ts",
      "import": "./dist/browser/ariaTree/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "bundle:aria": "tsx scripts/bundle-aria-tree.ts",
    "build": "pnpm run bundle:aria && tsc",
    "typecheck": "tsc --project tsconfig.check.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "spark": "NODE_NO_WARNINGS=1 tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "install-cli": "npm run build && chmod +x dist/cli.js && npm install -g .",
    "format": "prettier --write . --list-different",
    "format:check": "prettier --check .",
    "release": "./scripts/release.sh",
    "check": "pnpm run format && pnpm run typecheck && pnpm test"
  },
  "keywords": [
    "ai",
    "automation",
    "web-scraping",
    "playwright",
    "browser-automation",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.2.0",
    "@types/turndown": "^5.0.6",
    "@vitest/coverage-v8": "^4.0.18",
    "esbuild": "^0.27.3",
    "jsdom": "^28.0.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.20",
    "@ai-sdk/google-vertex": "^4.0.41",
    "@ai-sdk/openai": "^3.0.25",
    "@ai-sdk/openai-compatible": "^2.0.26",
    "@ghostery/adblocker-playwright": "^2.13.4",
    "@openrouter/ai-sdk-provider": "^2.1.1",
    "ai": "^6.0.69",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.3",
    "eventemitter3": "^5.0.4",
    "liquidjs": "^10.24.0",
    "nanoid": "^5.1.6",
    "ollama-ai-provider-v2": "^3.0.3",
    "playwright": "1.58.1",
    "turndown": "^7.2.2",
    "zod": "^4.3.6"
  },
  "packageManager": "pnpm@9.0.0"
}
