{
  "name": "@tabstack/spark",
  "version": "0.12.1",
  "description": "AI-powered web automation library and CLI tool",
  "type": "module",
  "engines": {
    "node": "^22.0.0"
  },
  "scripts": {
    "dev:server": "pnpm --filter spark-server dev",
    "dev:extension": "pnpm --filter spark-extension dev --",
    "clean": "pnpm -r run clean",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "pnpm -r run typecheck",
    "test": "pnpm -r run test",
    "check": "pnpm run format:check && pnpm -r run typecheck && pnpm -r run test",
    "release": "./scripts/release.sh"
  },
  "keywords": [
    "ai",
    "automation",
    "web-scraping",
    "playwright",
    "browser-automation",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@9.0.0"
}
